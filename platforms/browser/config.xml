<?xml version='1.0' encoding='utf-8'?>
<widget id="com.huzaifrangila.pettato" version="2.20.97" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pettato</name>
    <description>
        Social and Business Connectivity platform for pet owners and Pet Businesses
    </description>
    <author email="pettatoo@gmail.com" href="http://pettato.com">
        Pettato Enterprise LLP
    </author>
    <content src="index.html" />
    <icon src="app_icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://notchitup.in/*/*" />
    <allow-intent href="https://notchitup.in/*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="orientation" value="portrait" />
    <preference name="StatusBarStyle" value="default" />
</widget>
